{
  "name": "Sample",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Sample Chrome Extension",
  "content_scripts": [{
    "matches": [
      "https://qiita.com/*"
    ],
    "js": [
      "content.js"
    ]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "background"
  ]
}
